setwd("C://users//sala-c23//desktop")
require(foreign)
sdem215<-read.dbf("sdemt2016.dbf")
coe1t215<-read.dbf("coe1t2016.dbf")
coe2t215<-read.dbf("coe2t216.dbf")
folio<-paste(sdem215$CD_A, sdem2015$CON, sdem215$V_SEL, sdem215$N_HOG,sdem215$H_MUD, sdem215$N_REN )
x<-data.frame(sdem215,folio)
folio<-paste(coe1t215$CD_A, coe1t215$CON, coe1t215$V_SEL, coe1t215$N_HOG,coe1t215$H_MUD, coe1t215$N_REN )
x1<-data.frame(coe1t215,folio)
folio<-paste(coe2t215$CD_A, coe2t215$CON, coe2t215$V_SEL, coe2t215$N_HOG,coe2t215$H_MUD, coe2t215$N_REN )
x2<-data.frame(coe2t215,folio)
total<-merge(x,x1,by="folio",all.x=TRUE)
EnoeTotal<-merge(total,x2,by="folio",all.x=TRUE)
View(EnoeTotal)
ls(x)
install.packages("questionr")
require(questionr)
wtd.table(EnoeTotal$SEX, EnoeTotal$P1, weights=EnoeTotal$FAC)
table(EnoeTotal$SEX, EnoeTotal$P1)
